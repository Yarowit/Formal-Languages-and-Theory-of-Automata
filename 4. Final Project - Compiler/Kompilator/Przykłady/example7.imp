# zagnieżdżone pętle 
#	0 0 0
#	31000 40900 2222010
#	
#	1 0 2
#	31001 40900 2222012
# działa

PROGRAM IS
	a, b, c, i, j, k
IN
	READ a;
	READ b;
	READ c;
	i:=111091;
	WHILE i<=111110 DO
		j:=209;
		WHILE j>=200 DO
			k:=11;
			WHILE k<=20 DO
				a:=a+k;
				k:=k+1;
			ENDWHILE
			b:=b+j;
			j:=j-1;
		ENDWHILE
		c:=c+i;
                i:=i+1;
	ENDWHILE
	WRITE a;
	WRITE b;
	WRITE c;
END
