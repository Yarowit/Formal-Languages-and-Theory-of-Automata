# Silnia+Fibonacci
# ? 20
# > 2432902008176640000
# > 6765
# dzia≈Ça

PROGRAM IS
    f[100], s[100], i[100], n, j, k, l
IN
    READ n;
    f[0] := 0;
    s[0] := 1;
    i[0] := 0;
    f[1] := 1;
    s[1] := 1;
    i[1] := 1;
    j := 2;
    WHILE j <= n DO
        k := j - 1;
        l := k - 1;
	i[j] := i[k] + 1;
	f[j] := f[k] + f[l];
        s[j] := s[k] * i[j];
        j:=j+1;
    ENDWHILE
    WRITE s[n];
    WRITE f[n];
END
